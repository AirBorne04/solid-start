{
	"compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"moduleResolution": "node",
		"strict": true,
		"noUncheckedIndexedAccess": true,
		"allowSyntheticDefaultImports": true,
		"esModuleInterop": true,
		"jsx": "preserve",
		"jsxImportSource": "solid-js",
		"allowJs": true,
		"checkJs": true,
		"types": ["vinxi/client"],
		"isolatedModules": true,
		"outDir": "dist",
		"declaration": true,
		"skipLibCheck": true
	}
}
